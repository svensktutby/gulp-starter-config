{
  "name": "mobiup",
  "version": "0.1.0",
  "description": "Gulp starter config",
  "main": "gulpfile.js",
  "author": "Andrew Shved",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/svensktutby/gulp-starter-config.git"
  },
  "keywords": [
    "gulp",
    "es6",
    "babel",
    "eslint",
    "stylelint",
    "sass",
    "pug"
  ],
  "scripts": {
    "test": "npm run test:bem && npm run test:pug && npm run test:style && npm run test:js",
    "test:bem": "bemlint build/*.html --mod='--'",
    "test:pug": "glob-exec \"src/**/*.pug\" -- \"pug-lint {{files.join(' ')}}\"; exit 0",
    "test:style": "stylelint \"src/**/*.scss\" --syntax scss; exit 0",
    "fix:style": "stylelint \"src/**/*.scss\" --syntax scss --fix",
    "test:js": "eslint \"src/js/**/*.js\" \"src/blocks/**/*.js\"; exit 0",
    "serve": "static build",
    "build": "NODE_ENV=production gulp build",
    "start": "gulp",
    "deploy": "gulp deploy"
  },
  "lint-staged": {
    "*.{js,pug,md}": "editorconfig-cli",
    "*.pug": "pug-lint",
    "*.scss": "stylelint --syntax=scss",
    "*.js": "eslint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@htmlacademy/editorconfig-cli": "^1.0.0",
    "babel-loader": "^8.0.6",
    "bemlint": "^1.7.0",
    "browser-sync": "^2.26.7",
    "del": "^5.1.0",
    "gh-pages": "^2.2.0",
    "glob-exec": "^0.1.1",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^7.0.1",
    "gulp-cache": "^1.1.3",
    "gulp-cached": "^1.1.1",
    "gulp-changed": "^4.0.2",
    "gulp-clean-css": "^4.2.0",
    "gulp-debug": "^4.0.0",
    "gulp-eslint": "^6.0.0",
    "gulp-favicons": "^2.4.0",
    "gulp-filter": "^6.0.0",
    "gulp-group-css-media-queries": "^1.2.2",
    "gulp-if": "^3.0.0",
    "gulp-imagemin": "^7.1.0",
    "gulp-load-plugins": "^2.0.2",
    "gulp-notify": "^3.2.0",
    "gulp-plumber": "^1.2.1",
    "gulp-pug": "^4.0.1",
    "gulp-rename": "^2.0.0",
    "gulp-sass": "^4.0.2",
    "gulp-sass-glob": "^1.1.0",
    "gulp-stylelint": "^11.0.0",
    "gulp-svgstore": "^7.0.1",
    "gulp.spritesmith": "^6.11.0",
    "husky": "^4.2.1",
    "imagemin-mozjpeg": "^8.0.0",
    "imagemin-pngquant": "^8.0.0",
    "lint-staged": "^10.0.3",
    "merge-stream": "^2.0.0",
    "node-normalize-scss": "^8.0.1",
    "node-static": "^0.7.11",
    "pug-lint": "^2.6.0",
    "stylelint": "^12.0.0",
    "stylelint-config-sass-guidelines": "^6.2.0",
    "stylelint-order": "^4.0.0",
    "stylelint-scss": "^3.14.0",
    "stylelint-selector-bem-pattern": "^2.1.0",
    "through2": "^3.0.1",
    "vinyl-buffer": "^1.0.1",
    "vinyl-named": "^1.1.0",
    "webpack-stream": "^5.2.1"
  },
  "dependencies": {
    "jquery": "^3.4.1"
  }
}
